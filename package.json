{
  "name": "sample",
  "displayName": "sample",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onCommand:sample.noteSelection",
    "onCommand:sample.removeNote",
    "onCommand:sample.clearStorage"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sample.noteSelection",
        "title": "Note Here!"
      },
      {
        "command": "sample.clearStorage",
        "title": "Clear Storage"
      },
      {
        "command": "sample.removeNote",
        "title": "Remove Note!"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "sample.noteSelection"
        },
        {
          "when": "editorHasHoverProvider",
          "command": "sample.removeNote"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.64.0",
    "@vscode/test-electron": "^2.0.3",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2"
  }
}
